            "providers": [
                "Dbiz\\SpinWheelTool\\ToolServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Dang Nhu Hai",
            "email": "haidv@dbiz.vn",
            "homepage": "https://dbiz.vn",
            "role": "Developer"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
